{
  "name": "my-lang-emu",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production ELECTRON_START_URL=http://localhost:3000 electron .",
    "build": "tsc && vite build",
    "dev:vite": "vite",
    "dev:electron": "wait-on tcp:3000 && cross-env NODE_ENV=development ELECTRON_START_URL=http://localhost:3000 electron .",
    "dev": "concurrently -k \"npm run dev:vite\" \"npm run dev:electron\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@thenick775/mgba-wasm": "^1.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "wait-on": "^8.0.3"
  }
}
